{
	"name": "tempus-backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"start:local:debug": "export NODE_ENV=local && nest start --debug --watch",
		"start:local": "export NODE_ENV=local && nest start",
		"start:local:watch": "export NODE_ENV=local && nest start --watch",
		"prisma:generate:local": "dotenv -e .env.local -- npx prisma generate",
		"prisma:push:local": "dotenv -e .env.local -- npx prisma db push",
		"start:dev": "export NODE_ENV=dev && node dist/src/main",
		"start:migrate:dev": "export NODE_ENV=dev && npm run build && npm run start:dev && prisma migrate deploy",
		"start:prod": "export NODE_ENV=prod && node dist/src/main",
		"start:migrate:prod": "prisma migrate deploy && npm run start:prod",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "10.2.7",
		"@nestjs/config": "^3.2.0",
		"@nestjs/core": "10.2.7",
		"@nestjs/event-emitter": "^2.0.2",
		"@nestjs/microservices": "10.2.7",
		"@nestjs/platform-express": "10.2.7",
		"@nestjs/swagger": "^7.1.16",
		"@prisma/client": "^5.7.1",
		"amqp-connection-manager": "^4.1.14",
		"amqplib": "^0.10.3",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@nestjs/cli": "10.2.0",
		"@nestjs/schematics": "10.0.2",
		"@nestjs/testing": "10.2.7",
		"@types/express": "^4.17.13",
		"@types/jest": "29.5.0",
		"@types/node": "18.15.11",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"class-validator": "^0.14.0",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "29.5.0",
		"jsonwebtoken": "^9.0.2",
		"prettier": "^2.3.2",
		"prisma": "^5.7.0",
		"shallow-equal-object": "^1.1.1",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "29.0.5",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"typescript": "^4.7.4",
		"uuid": "^9.0.1"
	}
}
